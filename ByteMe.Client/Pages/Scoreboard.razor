<div class="border rounded p-4 w-full max-w-lg">
    <h2 class="text-2xl font-bold mb-4">Scores</h2>
    <ul>
        @foreach (var score in Scores)
        {
            <li>@score.Key: @score.Value</li>
        }
    </ul>
</div>

@code {
    [Parameter] public Dictionary<string, int> Scores { get; set; }
}